<idea-plugin>
    <extensions defaultExtensionNs="org.rust">
        <toolchainProvider implementation="org.rust.wsl.RsWslToolchainProvider"/>
        <addSdkProvider implementation="org.rust.wsl.sdk.add.RsAddWslSdkProvider"/>
        <sdkValidator implementation="org.rust.wsl.sdk.RsWslSdkValidator"/>
        <remoteSdkEditor implementation="org.rust.wsl.sdk.RsWslSdkEditor"/>
        <sdkFlavor implementation="org.rust.wsl.sdk.flavors.RsWslSdkFlavor"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <remote.credentialsType implementation="org.rust.wsl.RsWslCredentialsType"/>
        <remote.credentialsLanguageContribution implementation="org.rust.wsl.RsWslCredentialsContribution"/>
    </extensions>
</idea-plugin>
